  },
  "dependencies": {
    "@tabler/icons-react": "^3.11.0",
    "@tanstack/react-router": "^1.46.4",
    "@tanstack/router-devtools": "^1.45.11",
    "@wasm-fmt/web_fmt": "^0.1.10",
    "cjp": "^2.3.1",
    "clsx": "^2.1.1",
    "jotai": "^2.9.1",
    "json-schema-to-zod": "^2.4.0",
    "punycode": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.4.0",
    "ts-pattern": "^5.2.0",
    "twitter-text": "^3.1.0",
    "uqr": "^0.1.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@tanstack/router-vite-plugin": "^1.46.6",

    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/twitter-text": "^3.1.10",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.20",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "postcss": "^8.4.40",
    "postcss-import": "^16.1.0",
    "react-helmet-async": "^2.0.5",
    "shiki": "^1.12.1",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "vite": "^5.3.5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "biome check --write ./src/"
